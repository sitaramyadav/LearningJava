Complex Number
--------------
Representation
	{
		{(x0,y0),(x0,y1)},
		{(x1,y0),(x2,y2)}
	}
behaviour
	add
		m1= {
				{(x0,y0),(x0,y1)},
				{(x1,y0),(x2,y2)}
			}

		m2= {
				{(a0,b0),(a0,b1)},
				{(a1,b0),(a2,b2)}
			}
		m1+m2 =	{
					{
					{ [(x0,y0)+(a0,b0)]  [(x0,y1)+(a0,b1)] },

					{ [(x1,y0)+(a1,b0)]  [(x2,y2)+(a2,b2)] }
		}		
	multiplication
		m1= {
				{ (x0,y0)  (x0,y1) },
				{ (x1,y0)  (x2,y2) }
			}

		m2= {
				{(a0,b0),(a0,b1)},
				{(a1,b0),(a2,b2)}
			}
		m1 * m2 =	{
					
						{[((x0,y0)*((a0,b0))+((x0,y1)*(a1,b0))]    [((x1,y0)*(a0,b1))+((x2,y2)*(a2,b2))]},
						{[((x1,y0)*((a0,b0))+((x2,y2)*(a1,b0))]    [((x1,y0)*(a0,b1))+((x2,y2)*(a2,b2))]}
					}		